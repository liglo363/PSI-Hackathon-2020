<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.SATACTPage">
    
    <NavigationPage.TitleView>
        <Label Text="SAT/ACT" TextColor="AntiqueWhite" FontSize="35"/>
    </NavigationPage.TitleView>
    
    <ContentPage.Content>
        <StackLayout Margin="20,35,20,20">
            <Button Text="Testing Dates" BackgroundColor="Aquamarine" FontSize="25" Clicked="Testing_Dates_Button_Clicked"/>
            <Button Text="Study Websites/Materials" BackgroundColor="Aquamarine" FontSize="25" Clicked="StudyWebsitesMaterials_Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>